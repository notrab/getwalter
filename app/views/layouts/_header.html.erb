<header>
  <div class="wrapper">
    <h1 class="logo">
      <%= link_to 'Walter', root_path %>
    </h1>
    <% if signed_in? %>
      <%= link_to 'Edit Account', my_account_path %>
      <%= link_to 'Sign out', sign_out_path, method: :delete %>
    <% else %>
      <%= link_to 'Sign up', sign_up_path %>
      <%= link_to 'Sign in', sign_in_path %>
    <% end %>
  </div>
</header>
